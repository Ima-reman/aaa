anj x          # Declare variable x
ma x 10        # Assign 10 to x
anjima x       # Print the value of x

++ x           # Increment x
anjima x       # Print the updated value of x

* x 5          # Multiply x by 5
anjima x       # Print the value of x after multiplication

ma y 20        # Declare another variable y and assign 20
anjima y       # Print the value of y

/ y 4          # Divide y by 4
anjima y       # Print the updated value of y

adharsh x 15 anjima "x is now 15"  # Print if x equals 15

-- x           # Decrement x
anjima x       # Print the value of x

% x 3          # Modulus operation on x with 3
anjima x       # Print the value of x after modulus

anakha         # Exit the interpreter
